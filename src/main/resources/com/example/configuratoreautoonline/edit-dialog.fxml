<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.configuratoreautoonline.EditDateController">
    <children>
        <DatePicker fx:id="datePicker" layoutX="50.0" layoutY="50.0" />
        <Button fx:id="saveButton" layoutX="50.0" layoutY="100.0" text="Salva" onAction="#handleSaveButtonAction" />
        <Button fx:id="cancelButton" layoutX="150.0" layoutY="100.0" text="Annulla" onAction="#handleCancelButtonAction" />
    </children>
</AnchorPane>
